{
    "name": "weeb-end",
    "version": "1.0.0",
    "description": "Backend for parsing manga's",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "start": "node dist/server.js",
        "build": "tsc",
        "build:clean": "npm run clean && npm run build",
        "build:start": "npm run build:clean && npm run start",
        "clean": "rimraf dist/*",
        "test": "npm run build:clean && jest",
        "format": "prettier -w src",
        "dev:nodemon": "nodemon --watch src/ --exec \"npm run build:start\" --verbose -e ts",
        "dev": "ts-node-dev src/server.ts"
    },
    "keywords": [],
    "author": "Hubble",
    "license": "ISC",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "better-sqlite3": "^7.4.3",
        "cors": "^2.8.5",
        "deepmerge": "^4.2.2",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "glob": "^7.2.0",
        "jquery": "^3.6.0",
        "jsdom": "^17.0.0",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "mysql2": "^2.3.0",
        "node-fetch": "^2.6.5",
        "rand-token": "^1.0.1",
        "reflect-metadata": "^0.1.13",
        "typeorm": "^0.2.41"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/better-sqlite3": "^7.4.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/glob": "^7.1.4",
        "@types/jest": "^27.0.2",
        "@types/jquery": "^3.5.6",
        "@types/jsdom": "^16.2.13",
        "@types/jsonwebtoken": "^8.5.5",
        "@types/node": "^16.11.11",
        "@types/node-fetch": "^2.5.10",
        "@types/node-sass": "^4.11.2",
        "jest": "^27.2.4",
        "nodemon": "^2.0.12",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.4.1",
        "ts-jest": "^27.0.5",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.4.3"
    },
    "prettier": {
        "singleQuote": true,
        "tabWidth": 4,
        "printWidth": 120
    }
}
